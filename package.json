{
  "name": "next-typescript-tailwind-mdx-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "init-kit",
    "build": "NODE_ENV=production next build",
    "postbuild": "NODE_ENV=production next-sitemap && yarn docs-to-json",
    "start": "next start",
    "test": "jest --watch",
    "docs-to-json": "kit ./scripts/docs-to-json.js --trust",
    "nothing": ""
  },
  "dependencies": {
    "@johnlindquist/kit": "https://github.com/johnlindquist/kit#52edf72",
    "@next/mdx": "^11.1.0",
    "@octokit/rest": "^18.9.1",
    "@reach/dialog": "^0.16.0",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.1",
    "client-oauth2": "^4.3.3",
    "cookie": "^0.4.1",
    "date-fns": "^2.23.0",
    "dotenv-flow": "^3.2.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^4.1.17",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0",
    "lodash": "^4.17.21",
    "mdx-prism": "^0.3.4",
    "memoizee": "^0.4.15",
    "next": "^11.1.0",
    "next-seo": "^4.26.0",
    "next-sitemap": "^1.6.164",
    "next-svgr": "^0.0.2",
    "prism-react-renderer": "^1.2.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.0.0",
    "react-syntax-highlighter": "^15.4.4",
    "react-use": "^17.2.4",
    "rehype": "^12.0.0",
    "rehype-raw": "^6.0.0",
    "remark-code-titles": "^0.1.2",
    "remark-footnotes": "^4.0.1",
    "remark-slug": "^7.0.0",
    "sharp": "^0.29.0",
    "simplebar-react": "^2.3.5",
    "slugify": "^1.6.0",
    "tree-cli": "^0.6.7",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@mdx-js/loader": "^1.6.22",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.4.1",
    "@types/jsonfile": "^6.0.1",
    "@types/lodash": "^4.14.172",
    "@types/memoizee": "^0.4.6",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/react-syntax-highlighter": "^13.5.2",
    "babel-jest": "^27.0.6",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jsonfile": "^6.1.0",
    "lint-staged": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "yarn-upgrade-all": "^0.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx,md,mdx,js}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  }
}
